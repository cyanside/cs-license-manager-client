{
    "name": "cyanside/cs-license-manager-client",
    "description": "Plugin license manager client for WooCommerce.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cyanside\\LicenseManagerClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zabir",
            "email": "xaviranik.me@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3||^8.0"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-master",
        "phpcompatibility/php-compatibility": "*",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7"
    },
    "scripts": {
        "phpcs": [
            "vendor/bin/phpcs -p -s"
        ],
        "phpcbf": [
            "vendor/bin/phpcbf -p"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
